  "bugs": {
    "url": "https://github.com/domengabrovsek/aws-sentry-ts-layer/issues"
  },
  "homepage": "https://github.com/domengabrovsek/aws-sentry-ts-layer#readme",
  "dependencies": {
    "@types/aws-lambda": "^8.10.100",
    "@types/node": "^17.0.42",
    "axios": "^0.27.2",
    "dotenv": "^16.0.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.1",
    "typescript": "^4.7.3"
  }
}
