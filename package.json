{
  "name": "aws-sentry-ts-layer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf node-modules-axios-layer node_modules common-layer",
    "compile": "tsc",
    "build": "npm run clean && npm install && npm run compile && npm run build:axios && npm run build:common",
    "build:axios": "mkdir -p node-modules-axios-layer/nodejs && mv node_modules node-modules-axios-layer/nodejs/",
    "build:common": "mkdir -p common-layer/nodejs && mv dist/common/* common-layer/nodejs/",
    "deploy": "sam deploy --no-confirm-changeset --no-fail-on-empty-changeset --profile aws-sam-ts-test-user --config-env dev --resolve-s3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/domengabrovsek/aws-sentry-ts-layer.git"
  },
  "keywords": [],
